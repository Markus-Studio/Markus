type Product {
    name: string;
    price: f32;
}

type City: geo {
    name: string;
}

type Seller: user {
    name: string;
    last_name: string;
    city: City;
}

type Sell {
    seller: Seller;
    city: City;
    date: time;
}

type SellRecord {
    sell: Sell;
    product: Product;
    soldAt: u32; # Price at the time of sell.
    quantity: u32;
}

query hey($x: Product) {
    is(Product),
    sum(.price)
}