type Point {
    x: i32;
    y: i32;
}

type Rect {
    center: Point;
}

action x() {
    create Point {
        .x = 98,
        .y = 20
    };

    create Rect {
        .center = create Point {
            .x = 9,
            .y = 12
        }
    };

    update {
        is(Point)
    } {
        .x = 43 + 32
    };

}

query x() {
    is(Point),
    eq(.x, add(5, 3))
}