type User1: user {
  string name1;
}

type User2: user {
  string name2;
}

permission User1 {
  is(User1)
}

permission User2 {
  is(User2)
}

type Thing {
  string ownerName;
}

query test1@(User1) {
  is(Thing),
  eq(.ownerName, %user.name1)
}

query test2@(User2) {
  is(Thing),
  eq(.ownerName, %user.name2)
}